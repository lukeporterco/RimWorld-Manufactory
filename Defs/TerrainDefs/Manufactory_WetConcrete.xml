    <texturePath>Terrain/Surfaces/Manufactory_WetConcrete</texturePath>
    <color>(130,130,130)</color>
    <edgeType>Hard</edgeType>
    <renderPrecedence>70</renderPrecedence>
    <isPaintable>false</isPaintable>
