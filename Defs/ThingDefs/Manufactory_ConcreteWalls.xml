<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="Wall">
    <defName>Manufactory_WetConcreteWall</defName>
    <label>wet concrete wall</label>
    <description>A freshly poured concrete wall form. It will harden into a concrete wall over time.</description>
    <graphicData>
      <texPath>Things/Building/Manufactory/Walls/WetConcreteWall</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1,1)</drawSize>
      <color>(0.60,0.60,0.60)</color>
    </graphicData>
    <statBases>
      <MaxHitPoints>220</MaxHitPoints>
      <WorkToBuild>1400</WorkToBuild>
      <Flammability>0</Flammability>
      <Beauty>-1</Beauty>
    </statBases>
    <stuffCategories Inherit="False" />
    <costStuffCount Inherit="False">0</costStuffCount>
    <useStuffTerrainAffordance>false</useStuffTerrainAffordance>
    <costList>
      <Steel>5</Steel>
    </costList>
    <designationCategory>Structure</designationCategory>
    <modExtensions>
      <li Class="Manufactory.Curing.CuringSettingsExtension">
        <cureTicks>60000</cureTicks>
      </li>
    </modExtensions>
    <uiOrder>2000</uiOrder>
    <fillPercent>1</fillPercent>
  </ThingDef>

  <ThingDef ParentName="Wall">
    <defName>Manufactory_ConcreteWall</defName>
    <label>concrete wall</label>
    <description>A sturdy cured concrete wall.</description>
    <graphicData>
      <texPath>Things/Building/Manufactory/Walls/ConcreteWall</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1,1)</drawSize>
      <color>(0.70,0.70,0.70)</color>
    </graphicData>
    <statBases>
      <MaxHitPoints>260</MaxHitPoints>
      <WorkToBuild>1400</WorkToBuild>
      <Flammability>0</Flammability>
      <Beauty>-1</Beauty>
    </statBases>
    <stuffCategories Inherit="False" />
    <costStuffCount Inherit="False">0</costStuffCount>
    <useStuffTerrainAffordance>false</useStuffTerrainAffordance>
    <costList>
      <Steel>5</Steel>
    </costList>
    <designationCategory Inherit="False" />
    <designationHotKey Inherit="False" />
    <uiOrder>2001</uiOrder>
    <fillPercent>1</fillPercent>
  </ThingDef>
</Defs>
